{
  "scripts": {
    "build": "babel src -d lib",
    "start": "nodemon src/index.js --exec yarn build",
    "example": "yarn run babel-node src/example.js"
  },
  "dependencies": {
    "moment": "^2.22.1",
    "underscore": "^1.9.0"
  },
  "babel": {
    "plugins": [
      "transform-es2015-modules-umd"
    ]
